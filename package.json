{
   "name": "@silvermine/tauri-plugin-download",
   "version": "0.1.0",
   "description": "Background download plugin for Tauri applications",
   "type": "module",
   "types": "./dist-js/index.d.ts",
   "main": "./dist-js/index.cjs",
   "module": "./dist-js/index.js",
   "exports": {
      "types": "./dist-js/index.d.ts",
      "import": "./dist-js/index.js",
      "require": "./dist-js/index.cjs"
   },
   "files": [
      "dist-js",
      "permissions",
      "README.md",
      "LICENSE"
   ],
   "scripts": {
      "build": "rollup -c",
      "check-node-version": "check-node-version --npm 10.5.0",
      "commitlint": "commitlint --from 3521760",
      "eslint": "eslint .",
      "markdownlint": "markdownlint-cli2",
      "prepare": "rollup -c",
      "rust:lint": "cargo lint-clippy && cargo lint-fmt",
      "rust:lint:fix": "cargo fix-clippy && cargo fix-fmt",
      "standards": "npm run markdownlint && npm run rust:lint && npm run eslint && npm run commitlint",
      "tauri": "tauri"
   },
   "author": "Jeremy Thomerson",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/silvermine/tauri-plugin-download.git"
   },
   "devDependencies": {
      "@rollup/plugin-typescript": "11.1.6",
      "@silvermine/eslint-config": "github:silvermine/eslint-config-silvermine#9dc1394edb2a6a3cb27cb5345a9e55e4ee5d08a3",
      "@silvermine/standardization": "2.2.3",
      "@tauri-apps/api": "2.9.1",
      "@tauri-apps/cli": "2.9.5",
      "eslint": "8.57.0",
      "rollup": "4.40.0",
      "tslib": "2.8.1",
      "typescript": "5.4.5"
   },
   "peerDependencies": {
      "@tauri-apps/api": "2.9.1"
   }
}
